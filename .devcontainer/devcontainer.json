{
  "name": "Julia Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "forwardPorts": [8888],
  "postCreateCommand": "julia -e 'using Pkg; Pkg.instantiate()'",
  "customizations": {
    "vscode": {
      "extensions": [
        "julialang.language-julia",
        "ms-toolsai.jupyter"
      ]
    }
  }
}
